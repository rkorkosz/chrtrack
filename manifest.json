{
  "manifest_version": 2,

  "name": "RTrack",
  "description": "RTrack",
  "version": "0.0.1",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js/vendor/moment.min.js",
      "js/vendor/moment-duration-format.min.js",
      "js/background.js"
    ]
  },
  "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com; object-src 'self'",
  "permissions": [
    "http://rtrack.localhost:5000/"
  ]
}